<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ParkIT.Views.AddNotePage">
    <ContentPage.ToolbarItems>
        <ToolbarItem x:Name="TbAddNote" Text="save" Clicked="TbAddNote_Clicked"></ToolbarItem>
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="EntTi" Placeholder="Title"/>
            <Entry x:Name="EntDe" Placeholder="Description"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>